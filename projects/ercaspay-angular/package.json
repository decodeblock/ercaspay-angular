{
  "name": "@decodeblock/ercaspay-angular",
  "version": "1.0.3",
  "description": "An Angular library for Ercaspay payment checkout integration.",
  "main": "bundles/ercaspay-angular.umd.js",
  "module": "fesm2022/ercaspay-angular.mjs",
  "typings": "ercaspay-angular.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./ercaspay-angular.d.ts",
      "esm2022": "./fesm2022/ercaspay-angular.mjs",
      "umd": "./bundles/ercaspay-angular.umd.js"
    }
  },
  "scripts": {
    "build": "ng build ercaspay-angular --configuration production",
    "publish:package": "npm publish --access public"
  },
  "peerDependencies": {
    "@angular/common": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/router": "^17.3.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "keywords": ["angular", "payment", "ercaspay", "checkout", "library"],
  "author": "Decodeblock",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/decodeblock/ercaspay-angular"
  },
  "bugs": {
    "url": "https://github.com/decodeblock/ercaspay-angular/issues"
  },
  "sideEffects": false
}
